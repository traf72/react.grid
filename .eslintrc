{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
    
  },
  "rules": {
    "react/prop-types": 0,
    "prefer-const": 2
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}